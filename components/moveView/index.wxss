/* components/moveView/index.wxss */
.container {
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 2;
  pointer-events: none;

}

.move-view {
  position: absolute;
  pointer-events: auto;
  right: 20px;
  bottom: 140px;
  width: 100rpx;
  height: 100rpx;
  background: red;

}